{
    "name": "freedom-platform-dev-darwin",
    "version": "0.1.3",
    "bin": {
        "node": "./.sm/bin/node"
    },
    "mappings": {
        "node": [
            "archive",
            "http://nodejs.org/dist/v0.8.19/node-v0.8.19-darwin-x64.tar.gz",
            {
                "descriptor": {
                    "bin": {
                        "node": "./bin/node",
                        "npm": "./bin/npm"
                    }
                }
            }
        ]
    },
    "optionalMappings": {
        "brew": [
            "sm",
            "github.com/mxcl/homebrew/tarball/master",
            {
                "descriptor": {
                    "pm": "make",
                    "bin": {
                        "brew": "./bin/brew"
                    }
                }
            }
        ]
    }
}